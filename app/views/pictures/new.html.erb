<div class="header header-color">
  <%= render :partial => 'layouts/navbar' %>
</div>

<% if notice %>
  <div class="alert">
    <%= notice %>
  </div>
<% end %>

<div class="container-fluid">
  <% if user_signed_in? %>
    <h1>Add a picture (portrait: 720x1080 - landscape: 1620x1080)</h1>
    <%= render 'form_picture' %>
  <% end %>
</div>

<%= render :partial =>'layouts/footer'%>
